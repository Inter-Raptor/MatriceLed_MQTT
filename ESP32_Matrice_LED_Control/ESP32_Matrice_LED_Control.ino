// Modes disponibles
enum Mode { OFF, LOGO, ANIMATION, APPLICATION };
Mode mode = OFF; // Initialisation en mode off
void handleMessage(char* topic, byte* payload, unsigned int length);
void modeApplication(char* topic, byte* payload, unsigned int length);

void setup() {
  Serial.begin(9600);
  matrix.begin();
  matrix.setBrightness(255);  // 50
  connectToWiFi();
  mqttClient.setServer("192.168.1.*** l'adresse de votre broker MQTT", 1883);
  mqttClient.setCallback(handleMessage);
  connectToMQTT();
}

void connectToWiFi() {
  WiFi.begin("SSID", "PASSWORD");
  while (WiFi.status() != WL_CONNECTED) {
    delay(1000);
  }
}

void connectToMQTT() {
  while (!mqttClient.connected()) {
    mqttClient.connect("ESP32Client", "le nom d'utilisateur", "le mots de passe");
    mqttClient.subscribe("matriceled");
    mqttClient.subscribe("le nom de votre pc sous windows");
  }
}

void wait(unsigned long duration) {
  static unsigned long endTime = 0;
  if (endTime == 0) {
    endTime = millis() + duration;
  }
  if (millis() >= endTime) {
    endTime = 0;
  }
}

void modeOff() {
  // teindre tous les LED
  for (int i = 0; i < LED_COUNT; i++) {
    matrix.setPixelColor(i, BLACK);
  }
  matrix.show();
}

void modeLogo() {
  const uint32_t* image = logo; // Pointeur constant vers l'image de logo

  int fadeAmount = 5; // Variation de la luminosit  chaque tape
  int brightness = 255; // Luminosit initiale
  bool increasing = false; // Indique si la luminosit est en train d'augmenter ou de diminuer

  while (mode == LOGO) {
    // Afficher l'image de logo avec la luminosit actuelle
    for (int i = 0; i < LED_COUNT; i++) {
      uint32_t pixelColor = image[i];
      uint8_t r = (uint8_t)(pixelColor >> 16);
      uint8_t g = (uint8_t)(pixelColor >> 8);
      uint8_t b = (uint8_t)pixelColor;

      // Appliquer la luminosit
      r = (r * brightness) / 255;
      g = (g * brightness) / 255;
      b = (b * brightness) / 255;

      matrix.setPixelColor(i, matrix.Color(r, g, b));
    }
    matrix.show();

    // Faire varier la luminosit
    if (increasing) {
      brightness += fadeAmount;
      if (brightness >= 255) {
        increasing = false;
      }
    } else {
      brightness -= fadeAmount;
      if (brightness <= 0) {
        increasing = true;
      }
    }

    // Vrifier si un nouveau mode est demand
    mqttClient.loop();
    if (mode != LOGO) {
      break;
    }

    // Attendre un peu avant la prochaine tape
    delay(100);
  }
}

void modeApplication(char* topic, byte* payload, unsigned int length) {
  // Conversion du payload en chane de caractres
  char message[length + 1];
  for (int i = 0; i < length; i++) {
    message[i] = (char)payload[i];
  }
  message[length] = '\0';

  Serial.print("Payload converti : ");
  Serial.println(message);

  // Remplacement du caractre de sparation par un espace
  for (int i = 0; i < length; i++) {
    if (message[i] == '_') {
      message[i] = ' ';
    }
  }

  Serial.print("Mot-cl recherch : ");
  Serial.println(message);

  // Recherche du tableau correspondant au mot-cl
  uint32_t* image = NULL;
  for (int i = 0; i < sizeof(tableauNoms) / sizeof(tableauNoms[0]); i++) {
    if (strcmp(message, tableauNoms[i]) == 0) {
      image = tableauPointeurs[i];
      break;
    }
  }

  // Affichage de l'image correspondante ou image noire
  if (image != NULL) {
    for (int i = 0; i < LED_COUNT; i++) {
      matrix.setPixelColor(i, image[i]);
    }
  } else {
    for (int i = 0; i < LED_COUNT; i++) {
      matrix.setPixelColor(i, BLACK);
    }
  }
  matrix.show();
}






   
void modeAnimation() {
  Serial.println("Entering modeAnimation()");
  unsigned long animationStartTime = millis();
  unsigned long frameStartTime = millis();
  int currentAnimation = random(0, NUM_ANIMATIONS);
  uint32_t* currentFrames = animations[currentAnimation];
  int numFrames = NUM_FRAMES[currentAnimation];
  int currentFrame = 0;
  bool animationInProgress = true;

  while (mode == ANIMATION) {
    unsigned long currentTime = millis();

    if (animationInProgress) {
      if (currentTime - frameStartTime >= 150) { // Temps de chaque image de l'animation
        frameStartTime = currentTime;

        for (int i = 0; i < LED_COUNT; i++) {
          matrix.setPixelColor(i, currentFrames[currentFrame * LED_COUNT + i]);
        }
        matrix.show();
        currentFrame++;

        if (currentFrame >= numFrames) {
          currentFrame = 0;
        }
      }

      if (currentTime - animationStartTime >= 30000) { // Temps total de l'animation
        animationInProgress = false;
        for (int i = 0; i < LED_COUNT; i++) {
          matrix.setPixelColor(i, BLACK);
        }
        matrix.show();
        animationStartTime = currentTime + 100000; // Dlai de 5 minutes avant la prochaine animation
      }
    } else {
      if (currentTime >= animationStartTime) {
        animationInProgress = true;
        currentAnimation = random(0, NUM_ANIMATIONS);
        currentFrames = animations[currentAnimation];
        numFrames = NUM_FRAMES[currentAnimation];
        animationStartTime = currentTime;
      }
    }

    mqttClient.loop(); // Vrifier les messages MQTT

    // Vrifier si le mode a chang
    if (mode != ANIMATION) {
      break;
    }
  }
}










 

void handleMessage(char* topic, byte* payload, unsigned int length) {
  // Convertir la charge utile en chane
  String message = "";
  for (int i = 0; i < length; i++) {
    message += (char)payload[i];
  }

  // Dtecter le message MQTT en fonction du sujet
  if (strcmp(topic, "matriceled") == 0) {
    if (message == "mode off") {
      mode = OFF;
    } else if (message == "mode logo") {
      mode = LOGO;
    } else if (message == "mode animation") {
      mode = ANIMATION;
    } else if (message == "mode application") {
      mode = APPLICATION;
    }

    // Appeler la fonction correspondante en fonction du mode
    if (mode == OFF) {
      modeOff();
    } else if (mode == LOGO) {
      modeLogo();
    } else if (mode == ANIMATION) {
      modeAnimation();
    }
  } else if (strcmp(topic, "nom du pc") == 0) {
    // Si le mode est APPLICATION, appeler la fonction modeApplication
    if (mode == APPLICATION) {
      modeApplication(topic, payload, length);
    }
  }
}




void loop() {
  if (!mqttClient.connected()) {
    connectToMQTT();
  }
  mqttClient.loop();

  // Vrifier si le mode est ANIMATION et si l'heure actuelle est suprieure ou gale  nextAnimationTime
  if (mode == ANIMATION && millis() >= nextAnimationTime) {
    // Lancer une nouvelle animation alatoire
    int currentAnimation = random(0, NUM_ANIMATIONS);
    uint32_t* currentFrames = animations[currentAnimation];
    int numFrames = NUM_FRAMES[currentAnimation];

    for (int frame = 0; frame < numFrames; frame++) {
      for (int i = 0; i < LED_COUNT; i++) {
        matrix.setPixelColor(i, currentFrames[frame * LED_COUNT + i]);
      }
      matrix.show();
      delay(50);
    }

    // Afficher une image noire aprs l'animation
    for (int i = 0; i < LED_COUNT; i++) {
      matrix.setPixelColor(i, BLACK);
    }
    matrix.show();

    // Mettre  jour nextAnimationTime pour la prochaine animation dans 5 minutes
    nextAnimationTime = millis() + 300000;
  }
}
